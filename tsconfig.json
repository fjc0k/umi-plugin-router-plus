{
  "compilerOptions": {
    "strict": true,
    "target": "ES5",
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "preserveSymlinks": true,
    "newLine": "LF",
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": false,
    "baseUrl": ".",
    "outDir": "./lib",
    "lib": ["esnext"]
  }
}
